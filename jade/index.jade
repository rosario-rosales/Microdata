          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
              i.fa.fa-bars
            a.navbar-brand.animate(href='#')
              img(src='img/logo.png', alt='logo1')
          .navbar-collapse.collapse
            ul.nav.navbar-nav.navbar-right.animate
              li
                a(href='#home') Inicio
              li
                a(href='#nosotros') Nosotros
              li
                a(href='#servicios') Soluciones
              li
                a(href='#clients') Clientes
